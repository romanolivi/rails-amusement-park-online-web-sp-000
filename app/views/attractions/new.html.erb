<%= form_for(@attraction) do |a| %> 
    <label>Name</label><br>
    <%= a.text_field :name %><br> 

    <label>Height Requirement (in inches)</label><br>
    <%= a.number_field :min_height %><br>

    <label>How fun is it? (1-5 scale)</label><br>
    <%= a.number_field :happiness_rating %><br>

    <label>How nausea-inducing is it? (1-5 scale)</label><br>
    <%= a.number_field :nausea_rating %><br>

    <label>How many tickets does it cost?</label><br>
    <%= a.number_field :tickets %><br>

    <%= a.button "Create Attraction" %> 
<% end %> 